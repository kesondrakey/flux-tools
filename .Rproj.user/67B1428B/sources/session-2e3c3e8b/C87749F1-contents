# should return the path to the installed CITATION
system.file("CITATION", package = "fluxtools")

# now citation() will use your script
citation("fluxtools")


# from your package source dir
devtools::install_local("E:/GitHub/fluxtools",
                        build_vignettes = TRUE,
                        force = TRUE)


